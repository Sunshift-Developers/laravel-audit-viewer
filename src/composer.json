{
    "name": "sunshift/audits",
    "description": "A Laravel audits viewer",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Sunsfhit Dev",
            "email": "dev@sunshift.dev"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.4|^8.0",
        "laravel/framework": "^8.0",
        "owen-it/laravel-auditing" : "^13.4",
        "sunshift/audits": "dev-main" 
    },
    "autoload": {
        "psr-4": {
            "Sunshift\\Audits\\": "src/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/Sunshift-Developers/laravel-audit-viewer",
            "branch": "main"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Sunshift\\Audits\\AuditsViewerProvider"
            ]
        }
    }
}